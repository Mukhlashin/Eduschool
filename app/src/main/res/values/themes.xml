<resources>
    <!-- Base application theme. -->
    <style name="Theme.Eduschool" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/socialite</item>
        <item name="colorPrimaryVariant">@color/socialite_complimentary</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/socialite_darker</item>
        <item name="colorSecondaryVariant">@color/socialite_complimentary</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>